#include <stdio.h>

int main()
{
   int entery1, entery2;
   printf("enter 2 number: ");
   scanf("%d %d", &entery1, &entery2);
   printf("the odd numbers between %d and %d are: \n", entery1, entery2);
   for (int i = entery1 + 1; i < entery2; i++)
   {
      if (i % 2 != 0)
      {
         printf("%d ", i);
      }
   }
}
