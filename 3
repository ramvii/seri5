#include <stdio.h>
#include <math.h>

int main()
{
    int resualt, entery;
    while (1)
    {
        printf("enter number: ");
        scanf("%d", &entery);
        if (entery == 0)
        {
            break;
        }
        else
        {
            resualt += entery;
        }
    }
    printf("the sum of them are %d", resualt);
}
