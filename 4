#include <stdio.h>
#include <math.h>

int main()
{
    int entery1, entery2;
    printf("wellcome to the evil comparison");
    printf("\nenter 2 number for comparison: ");
    scanf("%d %d", &entery1, &entery2);
    if (entery1 > entery2)
    {
        printf("%d < %d", entery1, entery2);
    }
    if (entery1 == entery2)
    {
        printf("%d = %d", entery1, entery2);
    }
    if (entery1 < entery2)
    {
        printf("%d > %d", entery1, entery2);
    }
}
